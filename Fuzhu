#include <iostream>
using namespace std;

int main ()
{
  int a;
  int b1;
  int b2;
  int c;

  cout << "Zhengfangxing press 4;changfangxing press 22" << endl;
  cin >> c;

  if (c == 4)
    {
      cout << "Bianchang:" << endl;
      cin >> a;
      
      cout << "Mianji:" << a * a << endl;
    }
    
  if (c == 22)
    {
      cout << "Chang: and Kuang:" << endl;
      cin >> b1;
      cin >> b2;
      
      cout << "Mianji:" << b1*b2 << endl;
    }

  return 0;
}
